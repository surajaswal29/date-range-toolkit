{
  "name": "date-range-toolkit",
  "version": "1.0.0",
  "description": "A lightweight TypeScript library for handling date ranges, providing preset ranges and date-related utilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "date",
    "range",
    "toolkit",
    "typescript",
    "dates",
    "calendar",
    "preset",
    "date-range",
    "date-picker",
    "date-utils"
  ],
  "author": {
    "name": "Suraj Aswal",
    "url": "https://github.com/surajaswal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/surajaswal/date-range-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/surajaswal/date-range-toolkit/issues"
  },
  "homepage": "https://github.com/surajaswal/date-range-toolkit#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
